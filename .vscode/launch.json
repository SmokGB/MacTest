{
    "configurations": [
        {
            "name": "Launch and Debug Hosted Blazor WebAssembly App",
            "type": "blazorwasm",
            "request": "launch",
            "hosted": true,
            "program": "${workspaceFolder}/bin/Debug/<target-framework>/<project-name.dll>",
            "cwd": "${workspaceFolder}"
        },
        {
            "name": "C#: MacTest",
            "type": "dotnet",
            "request": "launch",
            "projectPath": "${workspaceFolder}/MacTest.csproj",
            "launchConfigurationId": "TargetFramework=;MacTest"
        }
    ]
}